# OrbitGuardAI Configuration
# Copy this file to .env and fill in your credentials

# ===== Space-Track.org Credentials =====
# Register at: https://www.space-track.org/auth/createAccount
SPACETRACK_USERNAME=your_username_here
SPACETRACK_PASSWORD=your_password_here

# ===== Redis Cache Configuration =====
# Redis connection URL
# Local: redis://localhost:6379/0
# With password: redis://:your_password@localhost:6379/0
# Redis Cloud: redis://default:password@hostname:port
REDIS_URL=redis://localhost:6379/0

# Cache TTL in hours (default: 24)
CACHE_TTL_HOURS=24

# ===== Performance Settings =====
# Enable/disable cache (true/false)
USE_CACHE=true

# Enable/disable async fetching (true/false)
USE_ASYNC=true

# Enable/disable Rust engine (true/false)
USE_RUST=false

# Maximum concurrent API requests (for async mode)
MAX_CONCURRENT_REQUESTS=10

# ===== Logging =====
# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO
